{
  "version": 2,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/webhook/plain",
      "dest": "/api/webhook/plain"
    }
  ],
  "functions": {
    "api/webhook/plain.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "debug"
  }
}
